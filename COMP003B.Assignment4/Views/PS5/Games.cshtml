@model IEnumerable<Game>
@{
    ViewData["Title"] = "Games Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
    <partial name="_Navigation" />
}

<h2>Games Page</h2>
<p>This are some of the best games to play on the PS5</p>

<div class="row">
    @foreach (var game in Model)
    {
        <div class="col-md-4">
            <partial name="_Card" model="game" />
        </div>
    }
</div>

